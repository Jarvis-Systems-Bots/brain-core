{
  "name": "jarvis-brain/core",
  "description": "The Brain Core for Agent Management",
  "keywords": ["jarvis", "brain", "core"],
  "type": "library",
  "version": "v0.0.1",
  "license": "MIT",
  "authors": [
    {
      "name": "Xsaven",
      "email": "xsaven@gmail.com",
      "role": "Developer/Maintainer"
    }
  ],
  "autoload": {
    "psr-4": {
      "BrainCore\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "BrainCore\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/brain-core",
    "bin/brain-script"
  ],
  "require": {
    "php": "^8.2",
    "illuminate/console": "^12.0",
    "illuminate/database": "^12.0",
    "illuminate/config": "^12.0",
    "illuminate/http": "^v12.0",
    "illuminate/bus": "^v12.0",
    "illuminate/collections": "^v12.0",
    "illuminate/contracts": "^v12.0",
    "illuminate/encryption": "^v12.0",
    "illuminate/filesystem": "^v12.0",
    "illuminate/hashing": "^v12.0",
    "illuminate/json-schema": "^v12.0",
    "illuminate/macroable": "^v12.0",
    "illuminate/log": "^v12.0",
    "illuminate/pagination": "^v12.0",
    "illuminate/process": "^v12.0",
    "illuminate/redis": "^v12.0",
    "illuminate/support": "^v12.0",
    "illuminate/validation": "^v12.0",
    "illuminate/view": "^v12.0",
    "composer/composer": "^2.8",
    "symfony/var-dumper": "^7.3",
    "yosymfony/toml": "^1.0",
    "bfg/dto": "^1.9",
    "symfony/var-exporter": "^7.3",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^11"
  }
}
